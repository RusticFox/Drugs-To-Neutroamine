<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--Luciferium Neutralization-->
  <RecipeDef>
    <defName>LuciferiumNeutralization</defName>
    <label>neutralizing luciferium</label>
    <description>neutralizing luciferium to produce Neutroamine.</description>
    <jobString>neutralizing drugs.</jobString>
      <workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
      <workSkill>Medicine</workSkill>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Brewing</soundWorking>
    <workAmount>5000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Luciferium</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Luciferium</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Neutroamine>30</Neutroamine>
    </products>
    <skillRequirements>
        <Medicine>6</Medicine>
        <Crafting>6</Crafting>
    </skillRequirements>
    <recipeUsers>
      <li>DrugLab</li>
    </recipeUsers>
    <researchPrerequisite>DrugNeutralization</researchPrerequisite>
  </RecipeDef>
